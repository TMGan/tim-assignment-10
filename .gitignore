# Java compiled output
*.class
*.jar
*.war
*.ear

# Build tool directories and files
/target/                # Maven build output
/build/                 # Gradle build output
/out/                   # Generic build output
*.iml                   # IntelliJ module files
pom.xml.versionsBackup  # Maven backup files
.gradle/                # Gradle configuration
gradlew                 # Gradle wrapper
gradlew.bat             # Gradle wrapper for Windows
build.gradle.lockfile   # Gradle lock files

# Dependency directories
/lib/                   # Local libraries (if not managed by build tools)
/libs/                  # Alternative local libraries folder
dependency-reduced-pom.xml  # Maven shaded artifact

# IDE files
/.idea/                 # IntelliJ IDEA
*.ipr                   # IntelliJ project files
*.iws                   # IntelliJ workspace files
/.vscode/               # Visual Studio Code
/.eclipse/              # Eclipse IDE
.classpath              # Eclipse classpath
.project                # Eclipse project
.settings/              # Eclipse settings

# Log files
*.log
logs/
*.log.*

# Temporary files
*.tmp
*.bak
*.swp
*~.nib
.DS_Store               # macOS-specific

# Test coverage and reports
/coverage/              # Test coverage reports
/test-output/           # Test output directories
jacoco.exec             # JaCoCo coverage
site/                   # Maven site reports

# Environment-specific files
.env                    # Environment variables
*.properties.local      # Local property files
/config/local/          # Local configuration

# Package and distribution files
/dist/                  # Distribution folders
/bin/                   # Binary output folders

# Dependency lock files (if not needed)
*.lock
